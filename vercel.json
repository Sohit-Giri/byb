{
    "version": 2,
    "builds": [
      {
        "src": "bookyourbook/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "bookyourbook/wsgi.py"
      }
    ]
  }
  